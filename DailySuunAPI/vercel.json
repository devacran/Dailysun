{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "index.js",
            "methods": ["POST", "GET", "PUT", "DELETE"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}
